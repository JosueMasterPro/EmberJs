<GeneralContainer>
  <nav class="navbar navbar-light bg-light">
    <form class="form-inline">
      <input name="search" id="search" class="form-control mr-sm-2" type="text" placeholder="Search">
      <button id="button" class="btn btn-outline-success my-2 my-sm-0" type="submit" {{on 'click' (fn this.search 'blue')}}>
        Search
        </button>
    </form>
  </nav>
  {{#each this.model as |product|}}
    <Product @product={{product}}/>
  {{/each}}
</GeneralContainer>